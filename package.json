{
  "name": "tree-sitter-gosu",
  "version": "0.1.0",
  "description": "Tree-sitter parser for the Gosu language",
  "repository": "https://github.com/tree-sitter/tree-sitter-gosu",
  "license": "MIT",
  "author": {
    "name": "Antigravity"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "gosu"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prebuildify": "^6.0.1",
    "tree-sitter": "github:ashan/tree-sitter-prebuilt",
    "tree-sitter-cli": "^0.25.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "tree-sitter": ">=0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "tree-sitter test",
    "compile-grammar": "tsc",
    "generate": "npm run compile-grammar && tree-sitter generate",
    "prepare": "npm run generate",
    "analyze": "ts-node scripts/analyze_failures.ts",
    "build": "node-gyp rebuild"
  }
}