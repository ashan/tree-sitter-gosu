{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gosu",
      "camelcase": "Gosu",
      "title": "Gosu",
      "scope": "source.gosu",
      "file-types": [
        "gsx",
        "gs",
        "gst"
      ],
      "injection-regex": "^gosu$",
      "class-name": "TreeSitterGosu"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the Gosu language",
    "authors": [
      {
        "name": "Antigravity"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-gosu"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}